<%= render 'partials/header', :locals => {:page => "/dashboard"} %>
<div class="container">
    <div class="row">
        ID: <%= @match.id %>
    </div>
    <div class="row">
        Day: <%= @match.human_date %>
    </div>
    <div class="row">
        Winner: <a href="/players/<%= @winner.id %>"><%= @winner.real_name %></a>
    </div>
    <div class="row">
        Loser: <a href="/players/<%= @loser.id %>"><%= @loser.real_name %></a>
    </div>
    <div class="row">
        <a href="/matches/<%= @match.id %>/edit">Edit</a>
    </div>
</div>
